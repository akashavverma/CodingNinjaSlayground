// print a pattern such that the first line contains 1 star, the next line contain 3 star, the next line contain 5 star and so on 
and the pattern must be symmetrical with the first star being printed.

#include <bits/stdc++.h> 

void printPattern(int n) {
	// Write your code here.
	for(int i=1;i<=n;i++){
		for(int j=1;j<=n-i;j++){
			cout<<" ";
		}
		for(int k=1;k<=(i*2-1);k++){
			cout<<"*";
		}
		cout<<endl;
	}
}
