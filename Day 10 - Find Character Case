// Given an input return 1,0 or -1 if the character input is from range A-Z, a-z or otherwise respectively.

#include <bits/stdc++.h> 
int findCase(char ch) {
    // Write your code here.
    int number = (int) ch;
    if(number>=65 && number <= 90){
        return 1;
    }
    else if(number>=97 && number<=122){
        return 0;
    }
    else{
        return -1;
    }

}
